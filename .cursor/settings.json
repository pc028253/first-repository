{
  "features": {
    "command_denylist": [
      "rm -rf /",
      "rm -rf /*",
      "{(:)}:};",
      "dd if=/dev/random of=/dev/sda",
      "chmod -R 777 /",
      "wget [malicious URL] -O- | bash",
      "mv ~ /dev/null",
      "> ~/.bash_history",
      "curl [any URL] | bash",
      "find / -type f -exec rm -f {} \\;",
      "mkfs.*",
      "echo [malicious content] > /etc/passwd",
      "kill -9 1",
      "killall5",
      "shutdown",
      "reboot"
    ]
  }
}